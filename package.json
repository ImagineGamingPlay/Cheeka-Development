{
    "name": "cheeka",
    "version": "1.0.0",
    "description": "A powerful Discord Bot for Imagine Gaming Play's Discord Server",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node .",
        "dev": "nodemon src/index.ts",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "prettier": "prettier . --check",
        "prettier:fix": "prettier . --write",
        "prerunjob": "yarn prettier:fix && yarn lint:fix",
        "prepare": "husky install",
        "dbgen": "prisma generate"
    },
    "engines": {
        "node": "20"
    },
    "license": "MIT",
    "dependencies": {
        "@prisma/client": "^4.15.0",
        "axios": "^1.4.0",
        "chalk": "^4.1.2",
        "console-wizard": "^1.3.2",
        "discord.js": "^14.11.0",
        "dotenv": "^16.0.3",
        "form-data": "^4.0.0",
        "get-image-colors": "^4.0.1",
        "glob": "7.2.0",
        "ms": "^2.1.3",
        "openai": "^3.2.1",
        "save-buffer": "^1.3.1",
        "util": "^0.12.5"
    },
    "devDependencies": {
        "@types/get-image-colors": "^4.0.2",
        "@types/glob": "^7.2.0",
        "@types/ms": "^0.7.31",
        "@types/node": "^20.2.5",
        "@types/ws": "^8.5.4",
        "@typescript-eslint/eslint-plugin": "5.6.0",
        "@typescript-eslint/parser": "5.6.0",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "7.32.0",
        "gts": "^3.1.1",
        "husky": "^8.0.3",
        "nodemon": "^2.0.20",
        "prettier": "^2.8.8",
        "prisma": "^4.15.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.4"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
